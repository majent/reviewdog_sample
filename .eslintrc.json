{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": [ 
    "custom-rules"
  ],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "warn",
    "custom-rules/no-destructuring-context": "warn"
  }
}